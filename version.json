{
  "version": "2.1.0-build.{height}",
  "publicReleaseRefSpec": [
    "^refs/heads/master$", // we release out of master
    "^refs/heads/dev$", // we release out of dev
    "^refs/heads/v\\d+\\.\\d+" // we also release branches starting with vN.N
  ],
  "nugetPackageVersion":{
    "semVer": 2
  },
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  }
}
