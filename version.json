{
  "version": "2.1.1",
  "publicReleaseRefSpec": [
    "^refs/heads/master$", // we release out of master
    "^refs/heads/dev$", // we release out of dev
    "^refs/heads/rel/\\d+\\.\\d+\\.\\d+" // we also release branches starting with rel/N.N.N
  ],
  "nugetPackageVersion":{
    "semVer": 2
  },
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  }
}
